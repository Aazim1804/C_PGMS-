#include <stdio.h>
int main()
{
    int a[3][3],i,j,s;
    printf("Enter the values to Array");
    for(i=0;i<3;i++){
        for(j=0;j<3;j++){
            printf("a[%d][%d]:",i,j);
            scanf("%d",&a[i][j]);
        }
    }
        printf("The matrix is: \n");
        for(i=0;i<3;i++)
        {
            for(j=0;j<3;j++)
            {
                printf("\t%d",a[i][j]);
            }
            printf("\n\n");
        }
        s=a[0][0]+a[1][1]+a[2][2];
        printf("Sum=%d",s);
        
        
}
